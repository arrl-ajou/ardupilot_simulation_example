          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>2.51e-06</iyy>
          <iyz>0</iyz>
          <izz>8.931e-05</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://RealSense_D435/meshes/realsense.dae</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name="visual">
        <pose>0.1 0.04 0.2 0 0 1.936519</pose>
        <geometry>
          <mesh>
            <uri>model://RealSense_D435/meshes/realsense.dae</uri>
            <submesh>
